
public class Switch {

	public static void main(String[] args) {
		
		char grade='C';
		
		switch(grade) {
		
		case 'A': System.out.println("Congrats !!! Very Good");
		case 'B': System.out.println("Very Good");
		case 'C': System.out.println("Good");
		case 'D': System.out.println("Work Hard");
		}

	}

}
