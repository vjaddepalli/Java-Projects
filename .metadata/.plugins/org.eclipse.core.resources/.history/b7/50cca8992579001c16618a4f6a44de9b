
public class Switch1 {

	public static void main(String[] args) {

		
		
		short b=50;
		
		final short v1=20;
		final short v2=40;
		final short v3=60;
		
		switch (b) {
		default:
			System.out.println("Invalid");
		case 20:
			System.out.println("a1");
			break;
		case 40:
			System.out.println("a2");
			break;
		case 60:
			System.out.println("a3");
			break;
		default:
			System.out.println("Invalid");

		}

	}

}
