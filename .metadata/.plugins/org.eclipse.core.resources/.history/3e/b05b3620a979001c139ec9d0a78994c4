
public class IntializeMain {

	public static void main(String[] args) {
		
		int a=30;
		float b=40.0F;
		
		double c;
		int d;
		
		c=a+b; //implicit type casting
		
		byte b1=90;
		byte b2=100;
		short s1=200;
		int res=b1+b2+s1;
		
		byte b3=10+100;
		
		final int f;
		f=25;
		
		System.out.println(f+1);

	}

}
