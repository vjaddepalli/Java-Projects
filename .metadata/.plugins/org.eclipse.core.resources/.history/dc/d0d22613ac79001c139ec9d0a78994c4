
public class Main1 {

	public static void main(String[] args) {
		
		int a=30;
		System.out.println(a);
		
		float b;
		boolean c=true;
		System.out.println(c);
		
		int[] marks;
		marks=new int[3];
		
		System.out.println(marks[0]);
		System.out.println(marks[1]);
		System.out.println(marks[2]);
		System.out.println(marks.length);

	}

}
