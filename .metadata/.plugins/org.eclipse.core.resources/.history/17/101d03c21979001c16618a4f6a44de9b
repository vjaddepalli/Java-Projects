
public class DataTypes {

	public static void main(String[] args) {
		
		byte v1=127; //-128 to 127 //1byte
		short v2=31000; // 2bytes  -32768...32767
		int v3=123456; //4 bytes
		long v4=218364754;
		
		char v5='A'; //2 bytes
		float v6=45000.00f; //4 bytes
		double v7=45000.00; //8 bytes
		boolean v8=true; // 1 bit
		
		System.out.println(Long.MIN_VALUE);

	}

}
