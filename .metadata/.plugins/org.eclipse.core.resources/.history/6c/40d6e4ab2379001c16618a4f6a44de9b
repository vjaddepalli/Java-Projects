
public class Switch {

	public static void main(String[] args) {

		char grade = 'C';

		switch (grade) {

		case 'A':
			System.out.println("Congrats !!! Very Good");
			break;
		case 'B':
			System.out.println("Very Good");
			break;
		case 'C':
			System.out.println("Good");
			break;
		case 'D':
			System.out.println("Work Hard");
			break;
		default:
			System.out.println("Invalid");

		}

	}

}
